{
  "name": "code-my-interview",
  "version": "1.0.0",
  "author":"Saransh Balyan",
  "description": "Code my Interview with help of AI (Stealth Mode)",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
  "appId": "com.example.codemyinterview",
  "productName": "CodeMyInterview",
  "mac": { "target": "dmg" },
  "win": { "target": "nsis" },
  "linux": { "target": "AppImage" },
  "files": [ "**/*" ],
  "publish": { "provider": "github" }
},
  "dependencies": {
    "axios": "^1.6.8",
    "screenshot-desktop": "^1.12.7"
  },
  "devDependencies": {
    "electron-builder": "^26.0.12",
    "electron": "^29.0.0"
  }
}
